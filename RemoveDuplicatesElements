public class RemoveDuplicate{

  public static void main(String[] args){

    int arr[] ={1,22,2,22,3,3,4,5,3,2,5,4,5}
    int len = arr.length;
    int temp[] = new int[arr.length];
    int j= 0;
    for(int i= 0;i<len-1;i++){
      if(arr[i]!=arr[i+1])
       temp[j++] = arr[i];
      
    }
    temp[j++] = arr[len-1];
    
  }
}
