public void moveZeros(int a[] , int n){
int count =0;
for(int i =0;i<n;i++){
if(a[i]!=0)
a[count++]=a[i];
while (count < n) 
arr[count++] = 0; 
    } 
  
public static void main (String[] args) 
    { 
        int arr[] = {1, 9, 8, 4, 0, 0, 2, 7, 0, 6, 0, 9}; 
        int n = arr.length; 
        moveZeros(arr, n); 
        System.out.println("Array after pushing zeros to the back: "); 
        for (int i=0; i<n; i++) 
        System.out.print(arr[i]+" "); 
    } 
} 


